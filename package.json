    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gsap": "^2.1.3",
    "postcss-import": "^12.0.1",
    "react": "^16.8.6",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-tilt": "^0.1.4",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
